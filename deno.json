{
  "$schema": "https://raw.githubusercontent.com/denoland/deno/v1.41.0/cli/schemas/config-file.v1.json",
  "version": "0.1.0",
  "tasks": {
    "start": "deno run --watch --allow-read --allow-write --allow-env --unstable-temporal --allow-net --allow-sys --allow-run main.ts serve --watch",
    "compile": "deno compile --target=$TARGET --allow-read --allow-write --allow-env --unstable-temporal --allow-net --allow-sys --allow-run --output gomi-$TARGET main.ts"
  },
  "imports": {
    "@deno/gfm": "jsr:@deno/gfm@^0.8.2",
    "@std/async": "jsr:@std/async@^1.0.2",
    "@std/cli": "jsr:@std/cli@^1.0.2",
    "@std/collections": "jsr:@std/collections@^1.0.5",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.0",
    "@std/encoding": "jsr:@std/encoding@^1.0.1",
    "@std/front-matter": "jsr:@std/front-matter@^1.0.0",
    "@std/fs": "jsr:@std/fs@^1.0.0",
    "@std/http": "jsr:@std/http@^1.0.0",
    "@std/path": "jsr:@std/path@^1.0.2"
  }
}
